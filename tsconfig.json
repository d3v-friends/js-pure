{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "ES5",
      "ES6"
    ],
    "module": "NodeNext",
    "baseUrl": ".",
    "paths": {
      "@js-pure/*": [
        "./ts/*",
        "./esm/*",
        "./cjs/*"
      ]
    },
    "allowJs": true,
    "checkJs": false,
    "inlineSourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "./ts/**/*.ts"
  ]
}
