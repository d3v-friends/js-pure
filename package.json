{
  "name": "@d3v-friends/js-pure",
  "version": "0.0.1",
  "scripts": {
    "build": "sh build.sh"
  },
  "dependencies": {
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "prettier": "^3.1.1"
  },
  "files": [
    "mjs",
    "cjs",
    "tsconfig.json",
    "package.json",
    "pnpm-lock.yaml"
  ],
  "type": "commonjs",
  "main": "./cjs/index.cjs",
  "types": "./cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./mjs/index..mjs",
      "require": "./cjs/index.cjs"
    },
    "./*": {
      "import": "./mjs/*",
      "require": "./cjs/*"
    }
  },
  "engines": {
    "node": "^20.10.0"
  }
}
