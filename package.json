{
  "name": "@d3v-friends/js-pure",
  "version": "0.0.1",
  "scripts": {
    "build": "sh build.sh"
  },
  "dependencies": {
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/node": "20.10.4",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1",
    "tsc-alias": "1.8.8"
  },
  "main": "./index.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./index.ts"
    },
    "./*": {
      "import": "./dist/mjs/*",
      "require": "./dist/cjs/*",
      "types": "./*"
    }
  },
  "engines": {
    "node": "20.10.0"
  }
}
