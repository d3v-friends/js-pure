{
  "name": "@d3v-friends/js-pure",
  "version": "0.0.1",
  "scripts": {
    "build": "sh build.sh"
  },
  "dependencies": {
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "prettier": "^3.1.1"
  },
  "files": [
    "esm",
    "cjs",
    "tsconfig.json",
    "package.json",
    "pnpm-lock.yaml"
  ],
  "type": "module",
  "main": "./esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.cjs",
      "types": "./ts/index.ts"
    },
    "./*": {
      "import": "./esm/*",
      "require": "./cjs/*",
      "types": "./ts/*"
    }
  },
  "engines": {
    "node": "^20.10.0"
  }
}
