{
  "name": "@d3v-friends/js-pure",
  "version": "0.0.1",
  "scripts": {
    "build": "sh build.sh"
  },
  "dependencies": {
    "ts-loader": "^9.5.1",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@types/node": "20.10.4",
    "prettier": "3.1.1"
  },
  "files": [
    "mjs",
    "cjs",
    "tsconfig.json",
    "package.json",
    "pnpm-lock.yaml"
  ],
  "main": "./mjs/index.mjs",
  "type": "module",
  "types": "./mjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./mjs/index..mjs",
      "require": "./cjs/index.cjs",
      "default": "./ts/index.ts"
    },
    "./*": {
      "import": "./mjs/*",
      "require": "./cjs/*",
      "default": "./ts/index.ts"
    }
  },
  "engines": {
    "node": "20.10.0"
  }
}
